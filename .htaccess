SetEnv REGISTER_GLOBALS 0
SetEnv MAGIC_QUOTES 1
SetEnv PHP_VER 5
SetEnv SESSION_USE_TRANS_SID 0
FileETag MTime Size

LimitRequestBody 1073741824

AddOutputFilterByType DEFLATE text/plain
AddOutputFilterByType DEFLATE text/html
AddOutputFilterByType DEFLATE text/xml
AddOutputFilterByType DEFLATE text/css
AddOutputFilterByType DEFLATE application/xml
AddOutputFilterByType DEFLATE application/xhtml+xml
AddOutputFilterByType DEFLATE application/rss+xml
AddOutputFilterByType DEFLATE application/javascript
AddOutputFilterByType DEFLATE application/x-javascript

## EXPIRES CACHING ##
<IfModule mod_expires.c>
ExpiresActive On
ExpiresByType image/jpg "access 1 year"
ExpiresByType image/jpeg "access 1 year"
ExpiresByType image/gif "access 1 year"
ExpiresByType image/png "access 1 year"
ExpiresByType image/webp "access 1 year"
ExpiresByType application/pdf "access 1 month"
ExpiresByType application/x-shockwave-flash "access 1 month"
ExpiresByType image/x-icon "access 1 year"
ExpiresByType font/woff "access 1 year"
ExpiresByType font/woff2 "access 1 year"
ExpiresDefault "access 1 year"

</IfModule>
## EXPIRES CACHING ##

RewriteEngine On

RewriteRule inc.rsStyle.css include/inc.style.css.php
RewriteRule inc.rsStyle1.css include/inc.style1.css.php
RewriteRule inc.rsJavaScript.js include/inc.javascript.js.php
RewriteRule ordini.xml downloadordini.php

RewriteRule ^([^/]+)/([^/]+)/([^/]+)/([^/]+).png include/inc.webp.php?or_img_to_webp=$1/$2/$3/$4.png
RewriteRule ^([^/]+)/([^/]+)/([^/]+)/([^/]+).jpg include/inc.webp.php?or_img_to_webp=$1/$2/$3/$4.jpg
RewriteRule ^([^/]+)/([^/]+)/([^/]+)/([^/]+).jpeg include/inc.webp.php?or_img_to_webp=$1/$2/$3/$4.jpeg

RewriteRule ^([^/]+)/googleecb4d4ca48a72b9c.html googleecb4d4ca48a72b9c.html [L]
RewriteRule ^([^/]+)/sitemap.xml sitemap.xml [L]

RewriteRule ^([^/]+)/carrello.html index.php?menid=-1&ecomm_riepilogo=1 [L]

RewriteRule ^([^/]+)/([^/]+)/pagamento-fallito.html index.php?paypal_cancel_return=$2 [L]
RewriteRule ^([^/]+)/([^/]+)/grazie.html index.php?paypal_return=$2 [L]
RewriteRule ^([^/]+)/grazie.html index.php [L]

RewriteRule ^([^/]+)/([^/]+).html rsRedirect.php?url=$2&lan=$1 [L]
RewriteRule ^([^/]+)/rsAction.php rsAction.php [L]
RewriteRule ^([^/]+)/inc.menu.php inc.menu.php [L]
RewriteRule ^([^/]+)/sitemap.xml sitemap.xml [L]
RewriteRule ^([^/]+)/sitemap_small.xml sitemap_small.xml [L]
RewriteRule ^(..)$ rsRedirect.php?url2=home [L]
RewriteRule ^(../)$ rsRedirect.php?url2=home [L]

RewriteCond %{QUERY_STRING} ^$
RewriteRule ^index.php$ rsRedirect.php?url2=home_base [L]